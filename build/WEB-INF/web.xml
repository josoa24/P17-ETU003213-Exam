<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="3.0">
<servlet>
    <servlet-name>PrevisionServlet</servlet-name>
    <servlet-class>src.main.java.model.servlet.PrevisionServlet</servlet-class>
</servlet>
<servlet>
    <servlet-name>DepenseServlet</servlet-name>
    <servlet-class>src.main.java.model.servlet.DepenseServlet</servlet-class>
</servlet>
<servlet>
    <servlet-name>GeneralServlet</servlet-name>
    <servlet-class>src.main.java.model.servlet.GeneralServlet</servlet-class>
</servlet>
<servlet>
    <servlet-name>MainServlet</servlet-name>
    <servlet-class>src.main.java.model.servlet.MainServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>PrevisionServlet</servlet-name>
    <url-pattern>/prevision</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>PrevisionServlet</servlet-name>
    <url-pattern>/addPrevision</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>DepenseServlet</servlet-name>
    <url-pattern>/depense</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>DepenseServlet</servlet-name>
    <url-pattern>/addDepense</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>GeneralServlet</servlet-name>
    <url-pattern>/listDepense</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/login</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/auth</url-pattern>
</servlet-mapping>
</web-app>